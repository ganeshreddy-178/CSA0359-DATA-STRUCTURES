#include<stdio.h>
int main()
{
	int a[2][2],b[2][2],i,j;
	printf("enter a matrix1");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	printf("enter second matrix:");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			scanf("%d",&b[i][j]);
			
		}
	}
		printf("displaying first matrix\n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("%d",a[i][j]," ");
		}
		printf(" \n ");
	
	}
		printf("display second matrix :\n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("%d",b[i][j]);
		}
		printf(" \n ");
	}
		printf("multiplaction of matrix\n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("%d",a[i][j]*b[i][j]);
		}
		printf(" \n ");
	
	}
	
}

